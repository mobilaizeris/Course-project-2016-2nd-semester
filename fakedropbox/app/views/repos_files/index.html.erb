<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @files.each do |repo_file| %>
      <tr>
        <td><%= repo_file.file.url %> </td>
        <td><%= link_to 'Show', repo_file %></td>
        <td><%= link_to 'Edit', edit_repos_file_path(repo_file) %></td>
        <td><%= link_to 'Destroy', repo_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>

</table>

<br>

<%= form_for @file, html: { multipart: true } do |form| %>
  <%= form.file_field :file %>
  <%= form.submit :Submit %>
<% end %>
